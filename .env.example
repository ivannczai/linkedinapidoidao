# Variáveis de Ambiente do Servidor
PORT=3001
JWT_SECRET=your_super_secret_jwt_key_change_this

# Variáveis do Banco de Dados PostgreSQL (do Docker)
DB_USER=admin
DB_HOST=localhost
DB_DATABASE=linkedin_manager
DB_PASSWORD=supersecretpassword
DB_PORT=5432

# URN do Perfil do LinkedIn a ser gerenciado (OBRIGATÓRIO)
# Exemplo: urn:li:person:abcdef123
LINKEDIN_USER_URN=COLOQUE_O_URN_DA_PESSOA_AQUI

# Credenciais da Aplicação do LinkedIn para OAuth 2.0
# Estas credenciais devem ser obtidas no LinkedIn Developer Portal
LINKEDIN_CLIENT_ID=SEU_CLIENT_ID_AQUI
LINKEDIN_CLIENT_SECRET=SEU_CLIENT_SECRET_AQUI
LINKEDIN_REDIRECT_URI=http://localhost:3001/api/auth/linkedin/callback

# Chave da API do Google Gemini
GEMINI_API_KEY=SUA_CHAVE_GEMINI_API_AQUI
